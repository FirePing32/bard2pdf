{
  "manifest_version": 3,
  "name": "Save To Drive",
  "description": "Save files from Google classroom to Drive",
  "key": "",
  "oauth2": {
      "client_id": "60154600471-c09dnivch9e90n557lvlv7n6qnrgr814.apps.googleusercontent.com",
      "scopes": [""]
  },
  "version": "1.0.0",
  "permissions": [
    "identity"
  ],
  "host_permissions": ["https://classroom.google.com/*"],
  "icons": {
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png",
      "128": "icons/icon-128x128.png"
  },
  "action": {
      "default_popup": "html/index.html"
  },
  "background": {
      "service_worker": "js/background.js"
  }
}